		write(a[i]);
		if (a[i]%2==0) {
			write(" is an even!\n");
		}else {
			write(" is an odd!\n");
		}
	}

	j=0;
	while (i<10) {
		j=j+a[i];
		if (a[i]>0)  ; else ;
		i=i+1;
	}
	write("Summary of the 10 integers is ");
	write(j);
	return 1;
}